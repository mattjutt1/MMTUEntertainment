# ---- Zammad Environment ----
# Copy to ops/env/.env.zammad (do not commit secrets)
POSTGRESQL_HOST=zammad-db
POSTGRESQL_PORT=5432
POSTGRESQL_USER=zammad
POSTGRESQL_PASS=zammad_secret
POSTGRESQL_DB=zammad
POSTGRESQL_DB_CREATE=true
DATABASE_URL=postgresql://zammad:zammad_secret@zammad-db:5432/zammad
REDIS_URL=redis://zammad-redis:6379
MEMCACHE_SERVERS=zammad-redis:6379
ELASTICSEARCH_ENABLED=false
RAILS_ENV=production